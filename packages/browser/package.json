{
  "name": "@pixzle/browser",
  "version": "0.1.4",
  "description": "Browser implementation of image fragmentation and restoration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:check": "tsc -p tsconfig.build.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "pixzle",
    "image",
    "fragmentation",
    "restoration",
    "shuffle",
    "obfuscation",
    "browser",
    "web"
  ],
  "author": "tuki0918",
  "repository": {
    "type": "git",
    "url": "https://github.com/tuki0918/pixzle.git",
    "directory": "packages/browser"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@pixzle/core": "workspace:*",
    "@tuki0918/seeded-shuffle": "^1.0.0"
  },
  "devDependencies": {
    "happy-dom": "^20.0.11",
    "typescript": "^5.7.2",
    "vitest": "^3.1.4"
  }
}
